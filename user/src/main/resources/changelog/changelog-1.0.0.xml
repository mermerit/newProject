<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
 ">

    <changeSet id="change_1" author="mera">
        <createTable tableName="user_entity">
            <column name="user_id" type="integer" autoIncrement="true">
                <constraints unique="true" primaryKey="true" nullable="false"></constraints>
            </column>

            <column name="name" type="java.lang.String"></column>

            <column name="user_name" type="java.lang.String"></column>

            <column name="password" type="java.lang.String"></column>

            <column name="get_user_with_course" type="integer" />

        </createTable>
    </changeSet>







</databaseChangeLog>